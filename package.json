{
  "name": "mannering-woocommerce-child",
  "version": "1.0.0",
  "description": "Hi. I'm a starter theme called mannering_music, or underscores, if you like. I'm a theme meant for hacking so don't use me as a Parent Theme. Instead try turning me into the next, most awesome, WordPress theme out there. That's what I'm here for.",
  "author": "Automattic Theme Team",
  "license": "GPL-2.0-or-later",
  "keywords": [
    "WordPress",
    "Theme"
  ],
  "homepage": "https://github.com/Automattic/mannering_music#readme",
  "repository": {
    "type": "git",
    "url": "git+https://github.com/Automattic/mannering_music.git"
  },
  "bugs": {
    "url": "https://github.com/Automattic/mannering_music/issues"
  },
  "devDependencies": {
    "@csstools/postcss-sass": "^5.0.1",
    "@wordpress/babel-preset-default": "^7.6.0",
    "@wordpress/scripts": "^24.6.0",
    "@wordpress/stylelint-config": "^21.5.0",
    "dir-archiver": "^2.1.0",
    "node-sass": "^8.0.0",
    "rtlcss": "^4.0.0",
    "stylelint-config-prettier": "^9.0.4",
    "stylelint-config-standard": "^29.0.0",
    "stylelint-config-standard-scss": "^6.1.0"
  },
  "rtlcssConfig": {
    "options": {
      "autoRename": false,
      "autoRenameStrict": false,
      "blacklist": {},
      "clean": true,
      "greedy": false,
      "processUrls": false,
      "stringMap": []
    },
    "plugins": [],
    "map": false
  },
  "scripts": {
    "watch": "node-sass sass/ -o ./ --source-map true --output-style expanded --indent-type tab --indent-width 1 -w",
    "compile:css": "node-sass sass/ -o ./ && stylelint '*.css' --fix || true && stylelint '*.css' --fix",
    "compile:rtl": "rtlcss style.css style-rtl.css",
    "lint:scss": "wp-scripts lint-style 'assets/css/*.scss'",
    "lint:js": "wp-scripts lint-js 'assets/js/*.js'",
    "bundle": "dir-archiver --src . --dest ../mannering_music.zip --exclude .DS_Store .stylelintrc.json .eslintrc .git .gitattributes .github .gitignore README.md composer.json composer.lock node_modules vendor package-lock.json package.json .travis.yml phpcs.xml.dist sass style.css.map yarn.lock"
  }
}
